<div class="container">
	<div class="row">
	</div>
	<div class="col-md-offset-1 col-md-10">
		{{#each menu}}
			{{#if menus}}
					{{#each menus}}
						{{#isEmpty pagina}}
						{{else}}
							<c:url var="pagina" value="{{pagina}}" />
							<a class="btn btn-primary" href="{{context}}{{pagina}}">{{descripcion}} </a>
						{{/isEmpty}}
					{{/each}}
			{{/if}}
		{{/each}}
	</div>
</div>